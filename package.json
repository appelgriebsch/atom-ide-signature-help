{
  "name": "atom-ide-signature-help",
  "main": "./lib/main.js",
  "version": "0.13.3",
  "description": "A replacement of the SignatureHelp functionality from the original Atom-IDE / Nuclide package developed by Facebook.",
  "keywords": [
    "atom-package",
    "atom-ide",
    "signature-help"
  ],
  "repository": "https://github.com/atom-ide-community/atom-ide-signature-help",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "bump": "ncu -u"
  },
  "package-deps": [
    "atom-ide-markdown-service",
    "busy-signal"
  ],
  "dependencies": {
    "atom-ide-base": "^2.1.1",
    "atom-package-deps": "^6.0.0"
  },
  "devDependencies": {
    "@types/atom": "^1.40.5",
    "@types/node": "^14.14.16",
    "atom-languageclient": "^1.0.5",
    "prettier": "^2.2.1",
    "npm-check-updates": "^10.2.5",
    "build-commit": "^0.1.1"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "signature-help": {
      "versions": {
        "0.1.0": "provideSignatureHelp"
      }
    }
  }
}
