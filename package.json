{
  "name": "atom-ide-signature-help",
  "main": "./lib/main.js",
  "version": "0.7.2",
  "description": "A replacement of the SignatureHelp functionality from the original Atom-IDE / Nuclide package developed by Facebook.",
  "keywords": [
    "atom-package",
    "atom-ide",
    "signature-help"
  ],
  "repository": "https://github.com/atom-ide-community/atom-ide-signature-help",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "bump": "ncu -u"
  },
  "package-deps": [
    "atom-ide-markdown-service",
    "busy-signal"
  ],
  "dependencies": {
    "atom-package-deps": "^6.0.0",
    "dompurify": "^2.0.12",
    "etch": "^0.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/atom": "^1.40.4",
    "@types/dompurify": "^2.0.2",
    "@types/node": "^14.0.24",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "atom-languageclient": "^0.9.9",
    "prettier": "^2.0.5",
    "npm-check-updates": "^7.0.2",
    "build-commit": "^0.1.1"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "signature-help": {
      "versions": {
        "0.1.0": "provideSignatureHelp"
      }
    }
  },
  "consumedServices": {
    "markdown-renderer": {
      "versions": {
        "1.0.0": "consumeMarkdownRenderer"
      }
    }
  }
}
