{
  "name": "atom-ide-signature-help",
  "main": "./dist/main.js",
  "version": "0.15.4",
  "description": "A replacement of the SignatureHelp functionality from the original Atom-IDE / Nuclide package developed by Facebook.",
  "keywords": [
    "atom-package",
    "atom-ide",
    "signature-help"
  ],
  "repository": "https://github.com/atom-ide-community/atom-ide-signature-help",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "test.format": "prettier . --check",
    "lint": "eslint . --fix",
    "test.lint": "eslint .",
    "clean": "shx rm -rf dist",
    "dev": "npm run clean && cross-env NODE_ENV=development rollup -c -w",
    "build": "npm run clean && cross-env NODE_ENV=production rollup -c",
    "bump": "ncu -u",
    "build-commit": "build-commit -o dist",
    "prepare": "npm run build"
  },
  "package-deps": [
    "atom-ide-markdown-service",
    "busy-signal"
  ],
  "dependencies": {
    "atom-ide-base": "^3.0.0",
    "atom-package-deps": "^7.2.3"
  },
  "devDependencies": {
    "@types/atom": "^1.40.10",
    "@types/node": "^15.12.2",
    "build-commit": "^0.1.4",
    "cross-env": "^7.0.3",
    "prettier-config-atomic": "^2.0.5",
    "rollup-plugin-atomic": "^2.3.2",
    "shx": "^0.3.3",
    "typescript": "^4.3.2"
  },
  "prettier": "prettier-config-atomic",
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "signature-help": {
      "versions": {
        "0.1.0": "provideSignatureHelp"
      }
    }
  }
}
